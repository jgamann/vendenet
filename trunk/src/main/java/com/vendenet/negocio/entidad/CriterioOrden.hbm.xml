<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.vendenet.negocio.entidad.CriterioOrden" table="criterioorden">
		<id name="id" column="CRITERIOORDEN_ID">
			<generator class="native"></generator>
		</id>
		<property name="name" column="CRITERIOORDEN_NAME"/>
		<property name="clave" column="CLAVE_ORDEN"/>
	</class>
</hibernate-mapping>