<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.vendenet.negocio.entidad.Busqueda" table="busqueda">
		<id name="id" column="BUSQUEDA_ID">
			<generator class="native"></generator>
		</id>
		<property name="name" column="BUSQUEDA_NAME"/>
		<property name="fechaBusqueda" column="FECHA_BUSQUEDA"/>
	</class>
</hibernate-mapping>