<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.vendenet.negocio.entidad.VisitaWeb" table="visitaweb">
		<id name="id" column="VISITAWEB_ID">
			<generator class="native"></generator>
		</id>
		<property name="name" column="VISITAWEB_NAME"/>
		<property name="fechaVisita" column="FECHA_VISITA"/>
		<property name="navegador" column="NAVEGADOR"/>
	</class>
</hibernate-mapping>