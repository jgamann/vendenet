<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.vendenet.negocio.entidad.Provincia" table="provincia" lazy="false">
		<id name="id" column="PROVINCIA_ID">
			<generator class="native"></generator>
		</id>
		<property name="name" column="PROVINCIA_NAME"/>
		<property name="namelong" column="PROVINCIA_NAME_LONG"/>
	</class>
</hibernate-mapping>