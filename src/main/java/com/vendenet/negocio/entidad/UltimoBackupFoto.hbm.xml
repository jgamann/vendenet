<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.vendenet.negocio.entidad.UltimoBackupFoto" table="ultimobackupfoto" lazy="false">
		<id name="id" column="ULTIMOBACKUPFOTO_ID">
			<generator class="native"></generator>
		</id>
		<property name="name" column="ULTIMOBACKUPFOTO_NAME"/>
		<property name="ultimoId" type="integer" column="ULTIMOID"/>
	</class>
</hibernate-mapping>