<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.vendenet.negocio.entidad.TipoAnuncio" table="tipoanuncio" lazy="false">
		<id name="id" column="TIPOANUNCIO_ID">
			<generator class="native"></generator>
		</id>
		<property name="name" column="TIPOANUNCIO_NAME"/>
	</class>
</hibernate-mapping>